<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/background">

<TextView
    android:id="@+id/txtNombre3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="45dp"
    android:layout_marginTop="27dp"
    android:text="DNI:"
    android:textAlignment="center"
    android:textColor="#ffffff"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/textView3"
    app:layout_constraintEnd_toStartOf="@+id/edtxDNI"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtNombre2" />

<TextView
    android:id="@+id/txtNombre4"
    android:layout_width="103dp"
    android:layout_height="32dp"
    android:layout_marginStart="15dp"
    android:layout_marginTop="27dp"
    android:layout_marginBottom="84dp"
    android:text="Direccion:"
    android:textAlignment="center"
    android:textColor="#ffffff"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/btnSolicitarRescatista"
    app:layout_constraintEnd_toStartOf="@+id/spinnerDirecciones"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView3" />

<TextView
    android:id="@+id/txtNombre2"
    android:layout_width="83dp"
    android:layout_height="30dp"
    android:layout_marginStart="30dp"
    android:layout_marginTop="27dp"
    android:text="Apellido:"
    android:textAlignment="center"
    android:textColor="#ffffff"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/txtNombre3"
    app:layout_constraintEnd_toStartOf="@+id/edtxApellido"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtNombre" />

<Button
    android:id="@+id/btnSolicitarRescatista"
    android:layout_width="379dp"
    android:layout_height="53dp"
    android:layout_marginStart="18dp"
    android:layout_marginTop="83dp"
    android:layout_marginEnd="15dp"
    android:layout_marginBottom="44dp"
    android:background="@drawable/botonredondeado"
    android:text="Solicitar paramedico"
    android:textColor="#ffffff"
    android:textSize="25dp"
    android:textStyle="bold"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/spinnerDirecciones" />

<TextView
    android:id="@+id/txtParaSolicitarRescatista"
    android:layout_width="216dp"
    android:layout_height="40dp"
    android:layout_marginStart="43dp"
    android:layout_marginTop="16dp"
    android:layout_marginEnd="98dp"
    android:gravity="center"
    android:text="Para "
    android:textAlignment="center"
    android:textColor="#ffffff"
    android:textSize="28dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/imgbAtrasSoliciarRescatista"
    app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="12dp"
        android:background="#ffffff"
        app:layout_constraintTop_toBottomOf="@+id/txtParaSolicitarRescatista"
        tools:layout_editor_absoluteX="-14dp" />

<TextView
    android:id="@+id/txtNombre"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="30dp"
    android:layout_marginTop="75dp"
    android:text="Nombre:"
    android:textAlignment="center"
    android:textColor="#ffffff"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/txtNombre2"
    app:layout_constraintEnd_toStartOf="@+id/edtxNombre"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/divider" />

<TextView
    android:id="@+id/textView3"
    android:layout_width="106dp"
    android:layout_height="52dp"
    android:layout_marginStart="17dp"
    android:layout_marginTop="27dp"
    android:text="Numero de telefono: "
    android:textAlignment="center"
    android:textColor="#ffffff"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/txtNombre4"
    app:layout_constraintEnd_toStartOf="@+id/edtxNumeroDeTelefono"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/txtNombre3" />

<EditText
    android:id="@+id/edtxNombre"
    android:layout_width="256dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="32dp"
    android:layout_marginTop="75dp"
    android:layout_marginEnd="16dp"
    android:background="@drawable/texto_deshabilitado"
    android:ems="10"
    android:enabled="false"
    android:inputType="textPersonName"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/edtxApellido"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/txtNombre"
    app:layout_constraintTop_toBottomOf="@+id/divider" />

<EditText
    android:id="@+id/edtxApellido"
    android:layout_width="256dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="32dp"
    android:layout_marginTop="27dp"
    android:layout_marginEnd="16dp"
    android:background="@drawable/texto_deshabilitado"
    android:ems="10"
    android:enabled="false"
    android:inputType="textPersonName"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/edtxDNI"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/txtNombre2"
    app:layout_constraintTop_toBottomOf="@+id/edtxNombre" />

<EditText
    android:id="@+id/edtxDNI"
    android:layout_width="256dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="57dp"
    android:layout_marginTop="27dp"
    android:layout_marginEnd="16dp"
    android:background="@drawable/texto_deshabilitado"
    android:ems="10"
    android:enabled="false"
    android:inputType="textPersonName"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/edtxNumeroDeTelefono"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/txtNombre3"
    app:layout_constraintTop_toBottomOf="@+id/edtxApellido" />

<EditText
    android:id="@+id/edtxNumeroDeTelefono"
    android:layout_width="256dp"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginTop="37dp"
    android:layout_marginEnd="16dp"
    android:background="@drawable/texto_deshabilitado"
    android:ems="10"
    android:enabled="false"
    android:inputType="textPersonName"
    android:textSize="20dp"
    app:layout_constraintBottom_toTopOf="@+id/spinnerDirecciones"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/textView3"
    app:layout_constraintTop_toBottomOf="@+id/edtxDNI" />

<Spinner
    android:id="@+id/spinnerDirecciones"
    android:layout_width="256dp"
    android:layout_height="32dp"
    android:layout_marginStart="21dp"
    android:layout_marginTop="40dp"
    android:layout_marginEnd="16dp"
    android:background="@color/White"
    app:layout_constraintBottom_toTopOf="@+id/btnSolicitarRescatista"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toEndOf="@+id/txtNombre4"
    app:layout_constraintTop_toBottomOf="@+id/edtxNumeroDeTelefono" />

<ImageButton
    android:id="@+id/imgbAtrasSoliciarRescatista"
    android:layout_width="40dp"
    android:layout_height="40dp"
    android:layout_marginStart="14dp"
    android:layout_marginTop="16dp"
    android:background="@drawable/flecha_atras"
    android:scaleType="centerInside"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/flecha_atras" />

</androidx.constraintlayout.widget.ConstraintLayout>